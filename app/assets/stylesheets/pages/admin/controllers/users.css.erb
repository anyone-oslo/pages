table.users {
  border-collapse: collapse;
  margin: -8px;
}
table.users tr {
  border-bottom: 1px solid #ddd;
}
table.users th {
  border-top: 1px solid #ddd;
  padding:  8px 32px 8px 8px;
  color: #666;
  background: #eee;
}

table.users td {
  padding:  8px 32px 0 8px;
  vertical-align: top;
}
table.users td h3 {
  margin-top: -2px;
}
table.users tr.adminuser td {
  background: #ffe;
}

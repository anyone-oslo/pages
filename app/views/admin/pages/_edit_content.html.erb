<% @page.template_config.enabled_blocks do |block_name, block_options| %>
  <%= page_block_field(f, block_name, block_options) %>
<% end %>

<% if !@page.new_record? && @page.template_config.value(:tags) %>
  <div class="field">
    <label>
      Tags
    </label>
    <%= tag_editor_for(f, @page) %>
  </div>
<% end %>

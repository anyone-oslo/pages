    "normalize.css": "^8.0.1",
    "postcss": "^8.4.23",
    "postcss-calc": "^8.2.4",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-preset-env": "^8.3.2",
    "postcss-url": "^10.1.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",

    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.35",
    "postcss-calc": "^9.0.1",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.1",
    "postcss-import-ext-glob": "^2.1.1",

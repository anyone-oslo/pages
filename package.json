    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.22",
    "cssnano": "^7.1.2",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "postcss": "^8.5.6",
    "postcss-calc": "^10.1.1",
    "postcss-cli": "^11.0.1",
    "postcss-image-inliner": "^7.0.1",
    "postcss-import": "^16.1.1",
    "postcss-import-ext-glob": "^2.1.1",

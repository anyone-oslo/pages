{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    ".": {
      "release-type": "ruby",
      "package-name": "pages_core",
      "version-file": "lib/pages_core/version.rb",
      "extra-files": [
        {
          "type": "json",
          "path": "package.json",
          "jsonpath": "$.version"
        },
        {
          "type": "generic",
          "path": "Gemfile.lock",
          "regex": "pages_core \\((?<version>.+?)\\)"
        }
      ]
    }
  }
}
